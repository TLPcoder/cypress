{
    "local": 8080,
    "downstreamMiddleware": [
        {
            "paths": ["/hello-world"],
            "middleware": "../middleware/downstream.js"
        }
    ],
    "upstreamMiddleware": [
        {
            "middleware": "../middleware/upstream.js"
        }
    ],
    "routes": {
        "/hello-world": {
            "methods": ["GET", "POST"],
            "middleware": "../apis/hello-world.js",
            "tasks": {
                "parallel": [
                    {
                        "methods": ["GET", "POST"],
                        "middleware": "../utils/taskparallel.js",
                        "tasks": {}
                    }
                ],
                "seriel": [
                    {
                        "methods": ["GET", "POST"],
                        "middleware": "../utils/task.js",
                        "tasks": {
                            "seriel": [
                                {
                                    "methods": ["GET", "POST"],
                                    "middleware": "../utils/task1.js",
                                    "tasks": {
                                        "seriel": [
                                            {
                                                "methods": ["GET", "POST"],
                                                "middleware": "../utils/task2.js"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        },
        "/cypress": {
            "methods": ["GET"],
            "middleware": "../apis/cypress-framework-api.js"
        }
    }
}